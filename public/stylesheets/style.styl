body
  background-color #000
  color: #fff
  font-family Georgia,Serif;
  font-size 20px
a
  color: #00B7FF

em
  font-family Georgia,Serif;

h1
  margin: 0
  font-size 50px
  font-family: Helvetica,Arial,Sans-Serif;
  text-transform: uppercase

footer
  position: absolute
  bottom: 0
  z-index: 5
  img
    float: left
    margin: 20px
  p
    float: left


section
  blockquote
    font-size: 45px
    width: 450px
    line-height: 1.5em
    text-align: center

    em
      clear: both
      color: #aaa
      float: right
      margin-right 20px

  ul
    margin-top 40px
    margin-left 100px
  li
    margin-bottom 20px
    font-size: 36px

